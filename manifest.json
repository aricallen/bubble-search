{
  "name": "Bubble Search",
  "version": "0.1",
  "description": "Managed search results via include and exclude lists of urls",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/icon24.png",
    "default_title": "Bubble Search",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
